{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": true,
  "trailingComma": "all",
  "arrowParens": "avoid",
  "tabWidth": 2,
  "endOfLine": "crlf",
  "printWidth": 180,
  "bracketSpacing": true,
  "singleQuote": true,
  "overrides": [
    {
      "files": ".prettierrc",
      "options": { "parser": "json" }
    },
    {
      "files": "*.vue",
      "options": { "printWidth": 200 }
    }
  ]
}
